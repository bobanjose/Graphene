<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Graphene.API</id>
    <version>$version$</version>
    <authors>RelayHealth</authors>
    <owners>McKesson/RelayHealth</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/bobanjose/Graphene/blob/master/README.md</projectUrl>
